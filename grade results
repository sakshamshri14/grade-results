#include <stdio.h>
int main()
{
int marks;
printf("Enter your mark ");
scanf("%d",&marks);
printf(" You entered %d", marks);

	if(marks>=85)
	{
	printf(" Grade A");
	}
	else if (marks>=70)
	{ 
		printf(" Grade B");
	}
	else if (marks>=55)
	{
		printf("Grade C");
	}
	else if (marks>=40)
	{
		printf("Grade D");
	}
	else if (marks<40)
	{
		printf("Grade F");
	}
return 0;
}
